{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start:dev": "NODE_ENV=development node build/Debug",
    "start:prod": "NODE_ENV=production node build/Release",
    "typecheck": "tsc --noEmit",
    "build:dev": "tsc",
    "build:prod": "tsc --project ./tsconfig.prod.json",
    "build": "npm-run-all build:dev build:prod",
    "watch:dev:src": "tsc --watch",
    "watch:dev:build": "NODE_ENV=development nodemon -q build/Debug",
    "watch:dev": "concurrently --timings -n source,build -c blue,yellow \"npm run watch:dev:src\" \"npm run watch:dev:build\"",
    "watch:prod:src": "tsc --watch --project ./tsconfig.prod.json",
    "watch:prod:build": "NODE_ENV=production nodemon -q build/Release",
    "watch:prod": "concurrently --timings -n source,build -c blue,green \"npm run watch:prod:src\" \"npm run watch:prod:build\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "concurrently": "^7.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
